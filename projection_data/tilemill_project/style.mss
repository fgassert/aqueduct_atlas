#lyr {
  polygon-opacity:1;
  polygon-gamma:.2;
  [ws4028cl="2.8x or greater decrease"] {polygon-fill:#0099CC; }
  [ws4028cl="2x decrease"] {polygon-fill:#73AFD1; }
  [ws4028cl="1.4x decrease"] {polygon-fill:#F8C7D9; }
  [ws4028cl="Near normal"] {polygon-fill:#DDDDDD; }
  [ws4028cl="1.4x increase"] {polygon-fill:#F8AB95; }
  [ws4028cl="2x increase"] {polygon-fill:#FF7351; }
  [ws4028cl="2.8x or greater increase"] {polygon-fill:#FF1900; }
  [ws4028cl="No data"] {polygon-fill:#4e4e4e; }
}